{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/brokerage",
      "destination": "/brokerage.html"
    },
    {
      "source": "/print",
      "destination": "/print.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/init-db",
      "destination": "/init-db.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}